{
  "name": "archieos-backend",
  "version": "1.0.0",
  "main": "index.js",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "_moduleAliases": {
    "@routes": "dist/routes",
    "@db": "dist/db",
    "@services": "dist/services",
    "@plugins": "dist/plugins"
  },
  "scripts": {
    "ios:set-lan": "bash ./scripts/set-ios-lan-ip.sh",
    "dev": "nodemon --watch src --ext ts,js --exec ts-node src/app.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/app.js",
    "test": "npm run test:cleanup-before && NODE_OPTIONS='--max-old-space-size=8192' vitest run; EXIT_CODE=$?; npm run test:cleanup-after; exit $EXIT_CODE",
    "test:watch": "NODE_OPTIONS='--max-old-space-size=8192' vitest",
    "test:cleanup-before": "[ \"$CI\" = \"true\" ] && exit 0 || pkill -f 'node.*vitest' || true",
    "test:cleanup-after": "[ \"$CI\" = \"true\" ] && exit 0 || pkill -f 'node.*vitest' || true",
    "test:check": "ps aux | grep -c '[v]itest' || echo 0",
    "test:cleanup": "bash scripts/cleanup-tests.sh",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:e2e:smoke:production": "PLAYWRIGHT_BASE_URL=${PLAYWRIGHT_BASE_URL:-https://operationscenter.archieos.ai} playwright test --project=production-smoke",
    "db:bootstrap": "ts-node scripts/bootstrap-localstack-ddb.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "e2e": "vitest run -t 'E2E Flow'",
    "openapi": "node -e \"console.log(JSON.stringify(require('./dist/openapi.js').buildOpenApiSpec()))\" > openapi.json",
    "openapi:check": "ts-node scripts/check-openapi.ts",
    "infra:up": "docker compose up -d localstack",
    "infra:down": "docker compose down",
    "infra:logs": "docker compose logs -f localstack",
    "infra:init": "NODE_ENV=local ts-node scripts/infra-init.ts",
    "infra:verify": "NODE_ENV=local ts-node scripts/infra-verify.ts",
    "start:local": "./scripts/start-local.sh",
    "start:stack": "bash ./scripts/startup.sh",
    "seed": "NODE_ENV=local ts-node scripts/seed.ts",
    "seed:from-fe": "NODE_ENV=local ts-node --transpile-only scripts/seed-from-frontend.ts",
    "seed:reset": "NODE_ENV=local ts-node scripts/seed-reset.ts",
    "test:llm": "NODE_ENV=local ts-node --transpile-only scripts/test-llm-classifier.ts",
    "test:openai": "NODE_ENV=local ts-node --transpile-only scripts/test-openai.ts",
    "test:gpt5": "NODE_ENV=local ts-node --transpile-only scripts/test-gpt5.ts",
    "test:slack-event": "NODE_ENV=development tsx scripts/send-test-slack-event.ts",
    "slack:listen": "NODE_ENV=local ts-node --transpile-only scripts/slack-live-listener.ts",
    "slack:poll": "NODE_ENV=local ts-node --transpile-only scripts/slack-poll-listener.ts",
    "intake:worker": "AWS_REGION=${AWS_REGION:-us-east-1} AWS_ACCESS_KEY_ID=${AWS_ACCESS_KEY_ID:-test} AWS_SECRET_ACCESS_KEY=${AWS_SECRET_ACCESS_KEY:-test} LOCALSTACK_ENDPOINT=${LOCALSTACK_ENDPOINT:-http://localhost:4566} NODE_ENV=${NODE_ENV:-local} ts-node --transpile-only --require tsconfig-paths/register scripts/intake-worker.ts",
    "backfill-claimed-by": "NODE_ENV=local ts-node scripts/backfill-claimed-by.ts",
    "dev:all": "npm-run-all --parallel dev frontend:intake-worker",
    "frontend:intake-worker": "npm-run-all --parallel --print-label frontend:start intake:worker",
    "frontend:start": "cd packages/frontend && npm run dev",
    "docker:build": "docker build -t archieos-app .",
    "docker:build:pull": "docker build --pull -t archieos-app .",
    "docker:up": "docker compose -f docker-compose.yml -f docker-compose.override.yml up -d --build",
    "docker:down": "docker compose down -v",
    "docker:scan": "docker scout quickview archieos-app && docker scout cves archieos-app --only-severity critical,high",
    "trivy:scan": "docker run --rm -v /var/run/docker.sock:/var/run/docker.sock aquasec/trivy image --no-progress --severity HIGH,CRITICAL archieos-app"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "Operations Center Backend (Node 20 + Fastify + Vitest + LocalStack)",
  "engines": {
    "node": ">=22 <23"
  },
  "overrides": {
    "cross-spawn": "^7.0.6",
    "fast-jwt": "^5.0.6",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "eslint": "^9.9.0"
  },
  "dependencies": {
    "ajv": "^8.17.1",
    "@aws-sdk/client-cloudwatch": "^3.917.0",
    "@aws-sdk/client-dynamodb": "^3.917.0",
    "@aws-sdk/client-s3": "^3.917.0",
    "@aws-sdk/client-secrets-manager": "^3.917.0",
    "@aws-sdk/client-sns": "^3.917.0",
    "@aws-sdk/client-sqs": "^3.917.0",
    "@aws-sdk/cloudfront-signer": "^3.916.0",
    "@aws-sdk/credential-providers": "^3.916.0",
    "@aws-sdk/lib-dynamodb": "^3.917.0",
    "@aws-sdk/s3-request-presigner": "^3.917.0",
    "@fastify/aws-lambda": "^6.1.1",
    "@fastify/compress": "^6.5.0",
    "@fastify/cors": "^8.5.0",
    "@fastify/jwt": "8.0.1",
    "@fastify/rate-limit": "^10.3.0",
    "@openai/agents": "^0.2.1",
    "@slack/bolt": "^3.17.0",
    "@slack/rtm-api": "^7.0.0",
    "@slack/web-api": "^7.12.0",
    "@swc/core": "^1.14.0",
    "aws-xray-sdk-core": "^3.5.3",
    "cookie": "^1.0.2",
    "csv-parse": "^6.1.0",
    "dotenv": "^17.2.2",
    "exponential-backoff": "^3.1.3",
    "fastify": "^4.29.1",
    "fastify-plugin": "^5.0.1",
    "google-auth-library": "^9.0.0",
    "jose": "^5.8.0",
    "module-alias": "^2.2.3",
    "node-fetch": "^3.3.2",
    "openai": "^6.1.0",
    "pino-std-serializers": "^7.0.0",
    "prom-client": "^15.1.3",
    "lodash": "^4.17.21",
    "ulid": "^3.0.1",
    "vite": "^7.1.12",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.0",
    "@types/aws-lambda": "^8.10.152",
    "@types/node": "^20.14.10",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "nodemon": "^3.1.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3",
    "shadcn": "^3.4.2",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.5.4",
    "@types/lodash": "^4.17.0",
    "vitest": "^3.2.4"
  },
  "experiments": {
    "typescript": true
  },
  "buildOptions": {
    "types": true
  }
}
